{
  "compilerOptions": {
    "target": "es5",
    "module": "es2020",
    "sourceMap": true,
    "outDir": "./dist",
    "moduleResolution": "Node",
    "noEmitOnError": true,
    "strict": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
